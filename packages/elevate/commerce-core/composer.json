{
    "name": "elevate/commerce-core",
    "description": "Complete e-commerce engine: cart, wishlist, orders, discounts, checkout, and payment processing for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Claudiu Bardan",
            "email": "your@email.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^12.0",
        "illuminate/database": "^12.0",
        "illuminate/http": "^12.0",
        "illuminate/contracts": "^12.0",
        "elevate/payments": "@dev",
        "elevate/shipping": "@dev"
    },
    "autoload": {
        "psr-4": {
            "Elevate\\CommerceCore\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Elevate\\CommerceCore\\CommerceCoreServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
