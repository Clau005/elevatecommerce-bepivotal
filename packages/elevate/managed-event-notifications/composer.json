{
    "name": "elevate/managed-event-notifications",
    "description": "Managed event notification system with customizable templates for customer and staff notifications",
    "type": "library",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/notifications": "^10.0|^11.0|^12.0",
        "illuminate/mail": "^10.0|^11.0|^12.0",
        "resend/resend-php": "^0.13"
    },
    "autoload": {
        "psr-4": {
            "Elevate\\ManagedEventNotifications\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Elevate\\ManagedEventNotifications\\ManagedEventNotificationsServiceProvider"
            ]
        }
    }
}
