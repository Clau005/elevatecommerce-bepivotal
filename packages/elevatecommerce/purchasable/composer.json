{
    "name": "elevatecommerce/purchasable",
    "description": "Cart, Wishlist, Order, and Checkout functionality for ElevateCommerce",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "ElevateCommerce\\Purchasable\\": "src/",
            "ElevateCommerce\\Purchasable\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0|^12.0",
        "stripe/stripe-php": "^13.0|^14.0|^15.0|^16.0|^17.0|^18.0",
        "srmklive/paypal": "^3.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "ElevateCommerce\\Purchasable\\PurchasableServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
