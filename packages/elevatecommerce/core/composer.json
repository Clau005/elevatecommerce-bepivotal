{
    "name": "elevatecommerce/core",
    "description": "Core package for ElevateCommerce",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "ElevateCommerce\\Core\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ElevateCommerce\\Core\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "phpunit/phpunit": "^11.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "ElevateCommerce\\Core\\CoreServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
