{
    "name": "elevatecommerce/editor",
    "description": "Visual page builder and template system for Laravel",
    "type": "library",
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "ElevateCommerce\\Editor\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ElevateCommerce\\Editor\\EditorServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
